<div class="modal fade" id="newboss" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span class="ion-ios-close-empty"></span>
                </button>
                <span class="pe-7s-airplay services-icon-2"></span>
                <h4 class="service-title">Research Hiring Manager</h4>
            </div>
            <div class="modal-body">
                <%= form_for @boss, url: new_boss_path do |f| %>
                <% if @boss.errors.any? %>
                <div id="error_explanation">
                  <h2>
                    <%= pluralize(@boss.errors.count, "error") %> prohibited
                    this user from being saved:
                  </h2>

                  <ul>
                    <% @boss.errors.full_messages.each do |msg| %>
                    <li><%= msg %></li>
                    <% end %>
                  </ul>
                </div>
                <% end %>
                <%= f.label :email %>
                <%= f.email_field :email, class:"form-control" %>
                <%= f.label :company %>
                <%= f.collection_select :descomp_id, Descomp.all, :id, :company, {prompt: true}, class:"form-control" %><br>
                <%= f.submit "Research Hiring Manager", class:"btn btn-primary" %>
                <% end %>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->

