                                                        <%= f.label :gender, "Gender" %>
                                                        <%= f.text_field :gender, class:"form-control" %>
                                                        <%= f.label :company, "Company" %>
                                                        <%= f.text_field :company, class:"form-control" %>
                                                        <%= f.label :linked, "Linkedin Profile" %>
                                                        <%= f.text_field :linked, class:"form-control" %>
                                                        <%= f.label :location %>
                                                        <%= f.text_field :location, class:"form-control" %>
                                                        <%= f.label :facebook %>
                                                        <%= f.text_field :facebook, class:"form-control" %>
                                                        <%= f.label :github_handle, "Github Handle" %>
                                                        <%= f.text_field :github_handle, class:"form-control" %>
                                                        <%= f.label :google, "GooglePlus Handle" %>
                                                        <%= f.text_field :google, class:"form-control" %>
                                                        <%= f.label :angellist, "Anglelist Handle" %>
                                                        <%= f.text_field :angellist, class:"form-control" %>
                                                        <%= f.label :about_handle, "About Handle" %>
                                                        <%= f.text_field :about_handle, class:"form-control" %>
                                                        <%= f.label :gravatar, "Gravatar Handle" %>
                                                        <%= f.text_field :gravatar, class:"form-control" %>
                                                        <%= f.label :gravatar_avatar, "Gravatar Avatar URL" %>
                                                        <%= f.text_field :gravatar_avatar, class:"form-control" %>
                                                        <%= f.label :twitter_avatar, "Twitter Avatar URL" %>
                                                        <%= f.text_field :twitter_avatar, class:"form-control" %>
                                                        <%= f.submit %>
                                                        <% end %>
                                                        <% end %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br>

                                    </div>
                                </div>
                                <% unless @boss.nil? %>
                                <% unless @boss.github_handle.nil? %>
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h2 class="panel-title">Github Past 10 Repos</h2>
                                    </div>
                                    <div class="panel-body">
                                      <div class="text-center">

                                        <div class="pie">
                                            <%= pie_chart(@boss.langcount[0]) %>
                                        </div>

                                        Github Handle : <%= @boss.github_handle %>
                                        </div>
                                        <ul class="list-group">
                                          <% unless @boss.langcount[0].nil? %>
                                            <% @boss.langcount[0].each do |r| %>
                                            <li class="list-group-item">
                                                <span class="badge"> %<%= (((r[1])/(@total).to_f)*100).round(2) %></span>
                                                <%= r[0] %>
                                            </li>
                                            <% end %>
                                            <% end %>
                                        </ul>

                                        <div class="Update" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel-info">
                                                <div class="panel-heading" role="tab" id="headingThree">
                                                    <h4 class="panel-title">
                                                        <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
                                                            Click Here to See Repo Links
                                                        </a>
