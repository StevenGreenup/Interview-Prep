                                    <%= boss.valve %><br>
                                <% else %>
                                    <% end %>

                                    <% end %><br>
                                </div>
                            </div>

                        </div>
                        <div class="reviewwrap">
                            <div class="twitter-center">

                                <div class="panel panel-default ">
                                    <div class="panel-heading">
                                        <% @descomp.bosses.each do |boss| %>
                                        <% if boss.twitter.present? %>
                                        <h2 class="panel-title">@<%= boss.twitter %>
                                            Timeline</h2>
                                    <% else %>
                                        <h2 class="panel-title">Twitter Timeline</h2>

                                        <% end %>
                                    </div>
                                    <div class="panel-body">
                                        <% @descomp.bosses.each do |boss| %>
                                        <% if boss.twitter.present? %>

                                        <div class="Update" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel-info">
                                                <div class="panel-heading" role="tab" id="headingTwo">
                                                    <h4 class="panel-title">
                                                        <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseone" aria-expanded="false" aria-controls="collapseone">
                                                            Click to see tweets liked by @<%= boss.twitter %>
                                                        </a>
                                                    </h4>
                                                </div>
                                                <div id="collapseone" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingone">

                                                    <div class="panel-body">

                                                        <a class="twitter-timeline" href="https://twitter.com/<%= boss.twitter %>/likes"></a>
                                                        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    <% else %>
                                        <% end %>

                                        <% if boss.twitter.present? %>

                                        <a class="twitter-timeline" href="https://twitter.com/<%= boss.twitter %>">Tweets Liked by @TwitterDev</a>
                                        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>
