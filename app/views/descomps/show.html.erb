                                            <%= @reviews["id"] %><br>
                                            <strong>Name :
                                            </strong>
                                            <%= @reviews["name"] %><br>
                                            <strong>Website :
                                            </strong>
                                            <%= @reviews["website"] %><br>
                                            <strong>Industry :
                                            </strong>
                                            <%= @reviews["industry"] %><br>
                                            <strong>Number Of Ratings :
                                            </strong>
                                            <%= @reviews["numberOfRatings"] %><br>
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h2 class="panel-title">Featured Review</h2>
                                </div>
                                <div class="panel-body">
                                    <div class="featuredReview">
                                        <strong>Job Title :
                                        </strong>
                                        <%= @reviews["featuredReview"]["jobTitle"] %><br>
                                        <strong>Location :
                                        </strong>
                                        <%= @reviews["featuredReview"]["location"] %><br>
                                        <strong>Headline :
                                        </strong>
                                        <%= @reviews["featuredReview"]["location"] %><br>
                                        <strong>Pros :
                                        </strong>
                                        <%= @reviews["featuredReview"]["pros"] %><br>
                                        <strong>Cons :
                                        </strong>
                                        <%= @reviews["featuredReview"]["cons"] %><br>
                                        <strong>Overall :
                                        </strong>
                                        <%= @reviews["featuredReview"]["overall"] %><br>
                                        <strong>Featured Review url :
                                        </strong>
                                        <%= link_to @reviews["featuredReview"]["attributionURL"] %><br>
                                    </div>
                                </div>
                            </div>

                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h2 class="panel-title">CEO Review</h2>
                                </div>
                                <div class="panel-body">
                                    <div class="reviewwrap">
                                        <div class="ceopic">

                                            <% if @reviews["ceo"].present? %>
                                            <% if image_tag @reviews["ceo"]["image"].nil? %>
                                        <% else %>
                                            <%= image_tag @reviews["ceo"]["image"]["src"] %><br>
                                            <% end %>
                                            <strong>CEO :
                                            </strong>
                                            <%= @reviews["ceo"]["name"] %><br>
                                        </div>

                                        <div class="ceoabout">
                                            <strong>Title :
                                            </strong>
                                            <%= @reviews["ceo"]["title"] %><br>
                                            <strong>Number Of Ratings :
                                            </strong>
                                            <%= @reviews["ceo"]["numberOfRatings"] %><br>
                                            <strong>Percentage Approve :
                                            </strong>
                                            <%= @reviews["ceo"]["pctApprove"] %>%<br>
                                            <strong>Percentage Disapprove :
                                            </strong>
                                            <%= @reviews["ceo"]["pctDisapprove"] %>%<br>
                                        <% else %>
                                            Not Available
                                            <% end %>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>

                    <div class="modal fade" id="newcomp" tabindex="-1" role="dialog">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span class="ion-ios-close-empty"></span>
                                    </button>
                                    <span class="pe-7s-airplay services-icon-2"></span>
                                    <h4 class="service-title">Edit Company Twitter</h4>
                                </div>
                                <div class="modal-body">

                                    <%= form_for @descomp, url: edit_company_path(id: @descomp.id) do |f| %>
                                    <%= f.label :twitter %>
                                    <%= f.text_field :twitter, class:"form-control" %>

                                    <%= f.submit "Edit Twitter Handle", class:"btn btn-primary" %>

                                    <% end %>
                                </div>
                            </div>
                            <!-- /.modal-content -->
                        </div>
                        <!-- /.modal-dialog -->
                    </div>
                    <!-- /.modal -->

                    <div role="tabpanel" class="tab-pane" id="settings">

                        <div class="manager">

                            <div class="panel panel-default ">
                                <div class="panel-heading">
                                    <h2 class="panel-title">Hiring Manager's Information</h2>
                                </div>
                                <div class="panel-body">

                                    <% @descomp.bosses.each do |boss| %>
                                    <% if boss.twitter_avatar.present? %>
                                    <%= image_tag boss.twitter_avatar, class:"avatar" %>
                                    <% elsif %>

                                    <% if boss.github_avatar.present? %>
                                    <%= image_tag boss.github_avatar, class:"avatar" %>
                                    <% elsif %>

                                    <% if boss.angel_avatar.present? %>
                                    <%= image_tag boss.angel_avatar, class:"avatar" %>
                                    <% elsif %>

                                    <% if boss.about_avatar.present? %>
                                    <%= image_tag boss.about_avatar, class:"avatar" %>
                                    <% elsif %>

                                    <% if boss.gravatar_avatar.present? %>
                                    <%= image_tag boss.gravatar_avatar, class:"avatar" %>
                                <% else %>
                                    <%= image_tag "default_large.png", class:"avatar"  %>
                                    <% end %><% end %><% end %><% end %><% end %><br>
                                    <% if boss.name.present? %>
                                    <strong>Name:
                                    </strong>
                                    <%= boss.name %><br>
                                <% else %>

                                    <% end %>
                                    <% if boss.location.present? %>
                                    <strong>Location:
                                    </strong>
                                    <%= boss.location %><br>
                                <% else %>
                                    <% end %>
                                    <% if boss.gender.present? %>
                                    <strong>Gender:
                                    </strong>
                                    <%= boss.gender %><br>
                                <% else %>

                                    <% end %>
                                    <strong>Email:
                                    </strong>
                                    <%= boss.email %><br>
                                    <% if boss.twitter.present? %>
                                    <strong>Twitter Handle:
                                    </strong>
                                    <%= boss.twitter %><br>

                                <% else %>
                                    <% end %>
                                    <% if boss.twitter_site.present? %>
                                    <%= boss.twitter_site %><br>
                                <% else %>
                                    <% end %>

                                    <% if boss.linked.present? %>
                                    <strong>Linkedin Profile:
                                    </strong>
                                    <%=link_to "linkedin.com/#{boss.linked}", "http://www.linkedin.com/#{boss.linked}" %><br>
                                <% else %>
                                    <% end %>
                                    <% if boss.twitter_bio.present? %>
                                    <strong>Twitter Bio:
                                    </strong>
                                    <%= boss.twitter_bio %><br>
                                <% else %>
                                    <% end %>
                                    <% if boss.twitter.present? %>
                                    <strong>Twitter Followers:
                                    </strong>
                                    <%= boss.twitter_followers %><br>
                                    <strong>Twitter Following:
                                    </strong>
