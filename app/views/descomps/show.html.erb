<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        <div class="container">

            <% if flash[:notice] %>
            <div class="notice"><%= flash[:notice] %></div>
            <% end %>

            <div>

                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active">
                        <a href="#home" aria-controls="home" role="tab" data-toggle="tab">Company Information Gathered</a>
                    </li>
                    <li role="presentation">
                        <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Company GlassDoor</a>
                    </li>
                    <li role="presentation">
                        <a href="#settings" aria-controls="settings" role="tab" data-toggle="tab">Hiring Manager Information Gathered</a>
                    </li>

                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="home">

                        <div class="contentcontainer1">
                            <div class="static-about">

                                <div class="panel panel-default ">
                                    <div class="panel-heading">
                                        <h2 class="panel-title">About</h2>
                                    </div>
                                    <div class="panel-body">
                                        <div class="about">
                                            <div class="text-center">

                                                <%= image_tag @descomp.logo %><br>

                                            </div>
                                            <% if @company2.present? %>
                                            <strong>Approximate employees:
                                            </strong><%= @company2["organization"]["approx_employees"] %><br>
                                            <strong>Founded:
                                            </strong><%= @company2["organization"]["founded"] %><br>
                                            <strong>Sector:
                                            </strong><%= @descomp.sector %><br>
                                            <strong>Industry group:
                                            </strong><%= @descomp.industrygroup %><br>
                                            <strong>Industry:
                                            </strong><%= @descomp.industry %><br>
                                            <strong>Subindustry:
                                            </strong><%= @descomp.subindustry %><br>
                                            <strong>Description:
                                            </strong><%= @descomp.description %><br>
                                        <% else %>
                                            <% end %>

                                        </div>
                                    </div>
                                </div>

                                <div class="panel panel-default contact-absolute">
                                    <div class="panel-heading">
                                        <h2 class="panel-title">Contact</h2>
                                    </div>
                                    <div class="panel-body">
                                        <div class="contact">

                                            <strong>Headquarter address:</strong>
                                            <% if @descomp.address.present? %>
                                            <%= @descomp.address %><br>
                                        <% else %>

                                            <% unless @company2.nil? %>

                                            <% @company2["organization"]["contact_info"]["addresses"].map do |address_hash|  %>
                                            <%= address_hash["address_line1"] %>,
                                            <%= address_hash["locality"] %>
                                            <%= address_hash["region"]["code"] %>,
                                            <%= address_hash["postal_code"] %>
                                            <% end %>
                                            <% end %>
                                            <% end %>

                                            <hr>
                                            <% unless @company2.nil? %>
                                            <%= link_to @company2["website"], @company2["website"] %><br>
                                            <% if @company2["organization"]["links"].nil? %>
                                        <% else %>
                                            <% @company2["organization"]["links"].map do |links_hash|  %>
                                            <%= link_to links_hash["url"], links_hash["url"] %><br>
                                            <% end %>
                                            <% end %>
                                            <% end %>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="panel panel-default companytwitter">
                                <div class="panel-heading">
                                    <h2 class="panel-title">Company Twitter</h2>
                                </div>
                                <div class="panel-body">
                                    <% if @descomp.twitter.nil? %>

                                    <%= form_for @descomp, url: edit_company_path(id: @descomp.id) do |f| %>
                                    <%= f.label :twitter %>
                                    <%= f.text_field :twitter, class:"form-control" %>
                                    <%= f.submit %>
                                    <% end %>
                                    <h2>The Twitter for the Company you're researching wasn't found, but here are some tweets mentioning
                                        <%= @descomp.company %>
                                    </h2>
                                    <% @client.search("#{@descomp.company}", result_type: "recent").take(10).collect do |t| %>

                                    <%= raw(@client.oembed(t.id).html) %>

                                    <% end %>

                                <% else %>
                                    <div class="company-twitter">
                                        <div class="Update" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel-info">
                                                <div class="panel-heading" role="tab" id="headingTwo">
                                                    <div class="text-center">

                                                        <h4 class="panel-title">

                                                            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#liked" href="#liked" aria-expanded="false" aria-controls="collapseTwo">
                                                                Click to see tweets liked by @<%= @descomp.twitter %>
                                                            </a>

                                                        </h4>
                                                    </div>
                                                </div>
                                                <div id="liked" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                                                    <div class="panel-body">

                                                        <a class="twitter-timeline" href="https://twitter.com/<%= @descomp.twitter %>/likes">Tweets Liked by @TwitterDev</a>
                                                        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

                                                    </div>
                                                </div>
                                            </div>

                                        </div>

                                        <a class="twitter-timeline" href="https://twitter.com/<%= @descomp.twitter %>"></a>
                                        <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

                                        <% if @descomp.twitter.nil? %>
                                    <% else %>
                                        <h2>Not the correct twitter?</h2>
                                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#newcomp">Edit Twitter Handle</button>
                                        <% end %>
                                    </div>
                                    <% end %>
                                </div>
                            </div>

                            <div class="aboutwrap">

                                <div class="panel panel-default social-absolute">
                                    <div class="panel-heading">
                                        <h2 class="panel-title">Social Profiles</h2>
                                    </div>
                                    <div class="social-panel-body">
                                        <div class="social-profiles">
                                            <% unless @company2.nil? %>
                                            <% @company2["social_profiles"].each do |t| %>

                                            <strong>
                                                <u><%= t["type_name"] %></u>
                                            </strong>
                                            <br>
                                            <% if t["bio"].present? %>
                                            <strong>Bio:</strong><br>
                                            <%= t["bio"] %><br>
                                        <% else %>
                                            <br>
                                            <% end %>
                                            <strong>Link to Profile:</strong><br>
                                            <%= link_to t["url"], t["url"] %><br>

                                            <hr>
                                            <% end %>
                                            <% end %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div role="tabpanel" class="tab-pane" id="profile">

                        <div class="text-center">
                            <%= image_tag @reviews["squareLogo"] %>

                        </div>

                        <div class="contentcontainer">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h2 class="panel-title">
                                        Review</h2>
                                </div>
                                <div class="panel-body">

                                    <div class="reviewwrap">

                                        <div class="aboutreview">
                                            <br>
                                            <strong>Overall Rating :
                                            </strong>
                                            <%= @reviews["overallRating"] %><br>
                                            <strong>Rating Description :
                                            </strong>
                                            <%= @reviews["ratingDescription"] %><br>
                                            <strong>Culture And Values Rating :
                                            </strong>
                                            <%= @reviews["cultureAndValuesRating"] %><br>
                                            <strong>Senior Leadership Rating :
                                            </strong>
                                            <%= @reviews["seniorLeadershipRating"] %><br>
                                            <strong>Compensation And Benefits Rating :
                                            </strong>
                                            <%= @reviews["compensationAndBenefitsRating"] %><br>
                                            <strong>Career Opportunities Rating :
                                            </strong>
                                            <%= @reviews["careerOpportunitiesRating"] %><br>
                                            <strong>Work Life Balance Rating :
                                            </strong>
                                            <%= @reviews["workLifeBalanceRating"] %><br>
                                            <strong>Recommend To Friend Rating :
                                            </strong>
                                            <%= @reviews["recommendToFriendRating"] %><br>
                                            <strong>Sector Id :
                                            </strong>
                                            <%= @reviews["sectorId"] %><br>
                                            <strong>Sector Name :
                                            </strong>
                                            <%= @reviews["sectorName"] %><br>
                                            <strong>Industry Id :
                                            </strong>
                                            <%= @reviews["industryId"] %><br>
                                            <strong>Industry Name :
                                            </strong>
                                            <%= @reviews["industryName"] %><br>
