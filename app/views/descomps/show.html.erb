                            <div class="boss-mentions update-absolute">

                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        <h2 class="panel-title">Update Information</h2>
                                    </div>
                                    <div class="panel-body">
                                        Please update hiring manager's Information if it's outdated or not currently present.<br><br>
                                        <div class="Update" id="accordion" role="tablist" aria-multiselectable="true">
                                            <div class="panel-info">
                                                <div class="panel-heading" role="tab" id="headingTwo">
                                                    <h4 class="panel-title">
                                                        <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                                                            Click Here to Update Information
                                                        </a>
                                                    </h4>
                                                </div>
                                                <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                                                    <div class="panel-body">

                                                        <% @descomp.bosses.each do |boss| %>
                                                        <%= form_for @boss, url: edit_boss_path(id: boss.id) do |f| %>
                                                        <%= f.label :twitter, "Twitter Handle" %>
                                                        <%= f.text_field :twitter, class:"form-control" %>
                                                        <%= f.label :name, "Full Name" %>
