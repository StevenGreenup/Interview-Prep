<%= wicked_pdf_stylesheet_link_tag "resumes" -%>



<div class="contact-resume">
  <h2><%= @current_user.name %></h2>
  <%=@resume.website %> - <%=@resume.linked %><br>
  <%= @resume.address %> <%=@resume.addresscity %>, <%=@resume.addressstate %> <%=@resume.addresszip %>- <%= @current_user.email %> - <%= @resume.phone %><br>




</div>

<div class="education">
  <h4><strong><u>Education</u></strong></h4>
  <br>
  <div class="school">
  &nbsp;<strong><%= @resume.school1 %></strong><br>
    &nbsp;<%= @resume.cdd1 %><br>
    <% if @resume.school2.present? %>
    &nbsp;<strong><%= @resume.school2 %></strong> <br>
    &nbsp;<%= @resume.cdd2 %><br>
    <% else %>
    <% end %>

    <% if @resume.school3.present? %>
    &nbsp;<strong><%= @resume.school3 %></strong> <br>
    &nbsp;&nbsp;<%= @resume.cdd3 %><br>
    <% else %>
    <% end %>
  </div>
</div>

<h4><strong><u>Relevant Experience</u></strong></h4>
